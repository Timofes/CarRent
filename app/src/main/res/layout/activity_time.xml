<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Login_Data_Registr"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/light_gray"
    tools:context=".menu.time.TimeActivity">

    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/timeshapez"
        app:layout_constraintBottom_toTopOf="@+id/imageButton3_time"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

    </FrameLayout>

    <EditText
        android:id="@+id/year_info_time"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:hint="Год: "
        android:singleLine="true"
        android:text=""
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintEnd_toStartOf="@+id/guideline75"
        app:layout_constraintStart_toStartOf="@+id/body_time_info_time"
        app:layout_constraintTop_toBottomOf="@+id/body_time_info_time" />

    <EditText
        android:id="@+id/addres_info_time"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:hint="Адрес:"
        android:singleLine="true"
        android:text=""
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintEnd_toStartOf="@+id/guideline75"
        app:layout_constraintStart_toStartOf="@+id/year_info_time"
        app:layout_constraintTop_toBottomOf="@+id/year_info_time" />

    <EditText
        android:id="@+id/description_info_time"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:hint="Описание:"
        android:lines="2"
        android:text=""
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="@+id/frameLayout2"
        app:layout_constraintStart_toStartOf="@+id/addres_info_time"
        app:layout_constraintTop_toBottomOf="@+id/addres_info_time" />

    <EditText
        android:id="@+id/body_time_info_time"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:hint="Кузов:"
        android:singleLine="true"
        android:text=""
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintEnd_toStartOf="@+id/guideline75"
        app:layout_constraintStart_toStartOf="@+id/Time_number_time"
        app:layout_constraintTop_toBottomOf="@+id/Time_number_time" />

    <TextView
        android:id="@+id/item_time1_info_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:background="@drawable/radiusshape2"
        android:padding="1dp"
        android:text="1"
        android:textColor="#ffffff"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/item_image_info_time"
        app:layout_constraintTop_toBottomOf="@+id/item_image_info_time" />

    <TextView
        android:id="@+id/item_time2_info_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/radiusshape2"
        android:paddingStart="2dp"
        android:paddingTop="1dp"
        android:paddingEnd="2dp"
        android:paddingBottom="1dp"
        android:text="1"
        android:textColor="#ffffff"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/item_time1_info_time"
        app:layout_constraintTop_toBottomOf="@+id/item_image_info_time" />

    <TextView
        android:id="@+id/item_time3_info_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/radiusshape2"
        android:paddingStart="2dp"
        android:paddingTop="1dp"
        android:paddingEnd="2dp"
        android:paddingBottom="1dp"
        android:text="1"
        android:textColor="#ffffff"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/item_time2_info_time"
        app:layout_constraintTop_toBottomOf="@+id/item_image_info_time" />

    <TextView
        android:id="@+id/item_time4_info_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/radiusshape2"
        android:paddingStart="2dp"
        android:paddingTop="1dp"
        android:paddingEnd="2dp"
        android:paddingBottom="1dp"
        android:text="1"
        android:textColor="#ffffff"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/item_time3_info_time"
        app:layout_constraintTop_toBottomOf="@+id/item_image_info_time" />

    <TextView
        android:id="@+id/item_time5_info_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/radiusshape2"
        android:paddingStart="2dp"
        android:paddingTop="1dp"
        android:paddingEnd="2dp"
        android:paddingBottom="1dp"
        android:text="1"
        android:textColor="#ffffff"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/item_time4_info_time"
        app:layout_constraintTop_toBottomOf="@+id/item_image_info_time" />

    <TextView
        android:id="@+id/item_time6_info_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/radiusshape2"
        android:paddingStart="2dp"
        android:paddingTop="1dp"
        android:paddingEnd="2dp"
        android:paddingBottom="1dp"
        android:text="1"
        android:textColor="#ffffff"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/item_time5_info_time"
        app:layout_constraintTop_toBottomOf="@+id/item_image_info_time" />

    <TextView
        android:id="@+id/item_time7_info_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/radiusshape2"
        android:paddingStart="2dp"
        android:paddingTop="1dp"
        android:paddingEnd="2dp"
        android:paddingBottom="1dp"
        android:text="1"
        android:textColor="#ffffff"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/item_time6_info_time"
        app:layout_constraintTop_toBottomOf="@+id/item_image_info_time" />

    <ImageView
        android:id="@+id/item_image_info_time"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/radiusshape1"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="@+id/frameLayout2"
        app:layout_constraintTop_toTopOf="@+id/frameLayout2"
        app:srcCompat="@drawable/ic_launcher_background" />

    <EditText
        android:id="@+id/item_Price_info_time"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:hint="Цена за час"
        android:singleLine="true"
        android:text=""
        android:textColor="#ffffff"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/frameLayout2"
        app:layout_constraintStart_toEndOf="@+id/item_image_info_time"
        app:layout_constraintTop_toBottomOf="@+id/editMarka_time" />

    <EditText
        android:id="@+id/item_name_info_time"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:hint="Марка"
        android:maxLines="1"
        android:singleLine="true"
        android:text=""
        android:textColor="#ffffff"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/frameLayout2"
        app:layout_constraintStart_toEndOf="@+id/item_image_info_time"
        app:layout_constraintTop_toTopOf="@+id/item_image_info_time" />

    <ImageButton
        android:id="@+id/imageButton2_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:background="@drawable/waitmenupress"
        android:src="@drawable/message_processing"
        android:transitionName="sms"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/guideline25"
        app:layout_constraintRight_toRightOf="@+id/guideline50"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imageButton1_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:background="@drawable/waitmenupress"
        android:src="@drawable/view_grid_outline"
        android:transitionName="grid"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@+id/guideline25"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imageButton3_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:background="@drawable/activemenupress"
        android:padding="7dp"
        android:transitionName="time"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/guideline50"
        app:layout_constraintRight_toRightOf="@+id/guideline75"
        app:srcCompat="@drawable/baseline_calendar_today_24"
        tools:ignore="SpeakableTextPresentCheck" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.25" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <ImageButton
        android:id="@+id/button_Right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:background="@drawable/radiusshape1"
        android:paddingHorizontal="20dp"
        android:paddingVertical="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/List_time"
        app:layout_constraintEnd_toEndOf="@+id/frameLayout2"
        app:layout_constraintStart_toEndOf="@+id/List_time"
        app:layout_constraintTop_toTopOf="@+id/List_time"
        app:srcCompat="@drawable/baseline_keyboard_arrow_right_24" />

    <TextView
        android:id="@+id/List_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="25dp"
        android:text="1/2"
        android:textColor="@color/light_gray"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/frameLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/button_Left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:background="@drawable/radiusshape1"
        android:paddingHorizontal="20dp"
        android:paddingVertical="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/List_time"
        app:layout_constraintEnd_toStartOf="@+id/List_time"
        app:layout_constraintStart_toStartOf="@+id/frameLayout2"
        app:layout_constraintTop_toTopOf="@+id/List_time"
        app:srcCompat="@drawable/baseline_keyboard_arrow_left_50" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline75"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#D9D9D9"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.75" />

    <ImageButton
        android:id="@+id/imageButton4_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:background="@drawable/waitmenupress"
        android:transitionName="profile"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@id/guideline75"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/account1"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/plus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:background="@drawable/plusshapez"
        app:layout_constraintEnd_toEndOf="@+id/frameLayout2"
        app:layout_constraintTop_toTopOf="@+id/frameLayout2"
        app:srcCompat="@drawable/plus" />

    <ImageView
        android:id="@+id/minus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/minusshapez"
        app:layout_constraintStart_toStartOf="@+id/frameLayout2"
        app:layout_constraintTop_toTopOf="@+id/frameLayout2"
        app:srcCompat="@drawable/minus" />

    <EditText
        android:id="@+id/Time_number_time"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:hint="Время в понедельник: "
        android:singleLine="true"
        android:text=""
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintEnd_toStartOf="@+id/guideline75"
        app:layout_constraintStart_toStartOf="@+id/item_time1_info_time"
        app:layout_constraintTop_toBottomOf="@+id/item_time4_info_time" />

    <EditText
        android:id="@+id/editMarka_time"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:hint="Модель"
        android:singleLine="true"
        android:text=""
        android:textColor="#ffffff"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/frameLayout2"
        app:layout_constraintStart_toStartOf="@+id/item_name_info_time"
        app:layout_constraintTop_toBottomOf="@+id/item_name_info_time" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Нет объявлений"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="28dp"
        android:visibility="invisible"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@id/frameLayout2"
        app:layout_constraintTop_toTopOf="@id/frameLayout2"/>

</androidx.constraintlayout.widget.ConstraintLayout>